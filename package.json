{ "name"            : "canopy"
, "description"     : "PEG parser compiler for JavaScript"
, "homepage"        : "http://canopy.jcoglan.com"
, "author"          : "James Coglan <jcoglan@gmail.com> (http://jcoglan.com/)"
, "keywords"        : ["parser", "compiler", "peg"]
, "license"         : "GPL-3.0"

, "version"         : "0.4.0"
, "engines"         : { "node": ">=0.4.0" }
, "files"           : ["bin", "lib"]
, "main"            : "./lib/canopy.js"
, "bin"             : { "canopy": "./bin/canopy" }

, "dependencies"    : { "mkdirp": ">=1.0.0", "nopt": "" }
, "devDependencies" : { "benchmark": "", "jstest": "", "pegjs": "" }

, "scripts"         : { "test": "find spec -name '*_spec.js' | xargs jstest" }

, "bugs"            : { "url": "https://github.com/jcoglan/canopy/issues" }

, "repository"      : { "type"  : "git"
                      , "url"   : "git://github.com/jcoglan/canopy.git"
                      }
}
